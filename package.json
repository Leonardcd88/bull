{
  "name": "bull",
  "version": "1.0.0",
  "description": "Job manager",
  "main": "./lib/queue",
  "repository": {
    "type": "git",
    "url": "git://github.com/OptimalBits/bull.git"
  },
  "keywords": [
    "job",
    "queue",
    "task",
    "parallel"
  ],
  "author": "OptimalBits",
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "bluebird": "^3.4.1",
    "debuglog": "^1.0.0",
    "lodash": "^4.13.1",
    "node-uuid": "^1.4.7",
    "redis": "^2.6.2",
    "semver": "^5.1.0"
  },
  "devDependencies": {
    "eslint": "^2.13.1",
    "expect.js": "^0.3.1",
    "mocha": "^2.5.3",
    "sinon": "^1.17.4"
  },
  "scripts": {
    "lint": "eslint lib test",
    "pretest": "npm run lint",
    "test": "mocha",
    "postpublish": "git push && git push --tags"
  },
  "eslintConfig": {
    "rules": {
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "valid-jsdoc": 0,
      "func-style": 0,
      "no-use-before-define": 0,
      "camelcase": 1,
      "no-unused-vars": 1,
      "no-alert": 1,
      "no-console": 1,
      "quotes": [
        2,
        "single"
      ],
      "no-underscore-dangle": 0
    }
  }
}
